<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
	"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
	"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
	<definition name="default" template="/WEB-INF/layouts/default.jsp">			
	</definition>
	
	<definition extends="default" name="home">
		<put-attribute name="body" value="/WEB-INF/views/home.jsp" />
	</definition>
	
	<definition extends="default" name="test">
		<put-attribute name="body" value="/WEB-INF/views/test/main.jsp" />
		<put-attribute name="message" cascade="true" value="/WEB-INF/views/message.jsp" />
	</definition>
</tiles-definitions>